{
  "manifest_version": 2,
  "name": "Enhanced Tracking Query Remover",
  "description": "Removes useless tracking information from url",
  "browser_specific_settings": {
    "gecko": {
      "id": "scott@entice.ca"
    }
  },
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "content.js" ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}



